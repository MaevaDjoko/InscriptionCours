<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>InscriptionCours</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>EtudiantControllers</servlet-name>
    <servlet-class>controllers.EtudiantControllers</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EtudiantControllers</servlet-name>
    <url-pattern>/etudiant</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>CoursControllers</servlet-name>
    <servlet-class>controllers.CoursControllers</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CoursControllers</servlet-name>
    <url-pattern>/creation</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>InscriptionControllers</servlet-name>
    <servlet-class>controllers.InscriptionControllers</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>InscriptionControllers</servlet-name>
    <url-pattern>/inscription</url-pattern>
  </servlet-mapping>

</web-app>